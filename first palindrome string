def first_palindromic_string(words):
    for word in words:
        if word == word[::-1]:
            return word
    return ""

# Example usage
print(first_palindromic_string(["abc", "car", "ada", "racecar", "cool"]))  # Output: "ada"
print(first_palindromic_string(["notapalindrome", "racecar"]))  # Output: "racecar"
